chat "ranger"
{
	//the teamplay.h file is included for all kinds of teamplay chats
	#include "teamplay.h"

	//======================================================
	//======================================================

	type "game_enter" //initiated when the bot enters the game
	{
		HELLO7;
		"Special orders for ", 4 ,": ... 'Remove all hostiles!'";
